// room script file

function room_AfterFadeIn()
{
		//move characters in
		int moved = 0;
		if(scorealien > 0) {
			cAlien.ChangeRoom(7);
			moved++;
		}
		if(scorebutler > 0) {
			cButler.ChangeRoom(7);
			moved++;
		}
		if(scoreshy > 0) {
			cShy.ChangeRoom(7);
			moved++;
		}
		if(scorespanx > 0){
			cSpanx.ChangeRoom(7);
			moved++;
		}
		if(scorestrong > 0) {
			cStrong.ChangeRoom(7);
			moved++;
		}
		
		if(moved == 0 && scoreevil > 0) {
			cEvil.ChangeRoom(7);
			moved++;
		}
		//dFinalDialogTest.Start();
		if(moved == 0) {
			//aloneending
			dEndingAlone.Start();
		}
}

function room_RepExec()
{
	if(cButler.Room == 7 || cShy.Room == 7 || cAlien.Room == 7 || cSpanx.Room == 7 ||
	cStrong.Room == 7 || cEvil.Room == 7) {
		//keep up
	} else {
		dEndingAlone.Start();
	}
}
